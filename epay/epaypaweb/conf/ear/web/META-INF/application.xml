<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd">

  <display-name>${product-name}</display-name>
  <module>
    <ejb>${business-module-name}.jar</ejb>
  </module>
  <module>
    <ejb>${facade-module-name}.jar</ejb>
  </module>
  <module>
	<web>
      <web-uri>${web-module-name}.war</web-uri>
      <context-root>/${context-path}</context-root>
      <!-- context-root>/epaypawebmigr</context-root -->
	</web>
  </module>

</application>
