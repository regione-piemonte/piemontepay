        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="JDBC Request Caricamento Dati Creazione Pagamenti" enabled="true">
          <stringProp name="dataSource">EPAY_JDBC_POOL</stringProp>
          <stringProp name="query">select 
	&apos;${codiceChiamante}&apos; as &quot;codice_chiamante&quot;,
	&apos;${pwd}&apos; as &quot;pwd&quot;,
	p.iuv_numero_avviso as iuv,
	anag.codice_fiscale as codice_fiscale,
	p.codice_pagamento_rif_ente as identificativo_pagamento,
	ente.codice_fiscale as codice_fiscale_ente
from epay_t_pagamento p
join epay_t_rt r on (p.iuv_numero_avviso = r.iuv)
join epay_t_anagrafica anag on (p.id_anagrafica_pagatore = anag.id_anagrafica)
join epay_t_tipo_pagamento tpag on (p.id_tipo_pagamento = tpag.id_tipo_pagamento)
join epay_t_enti ente on (tpag.id_ente = ente.id_ente)
where 
1=1
and p.codice_pagamento_rif_ente is not null
and p.id_stato_corrente in (4,3)
and tpag.pagamento_spontaneo = false
order by p.data_inserimento DESC
limit 1000</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
