<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="provvisorioDTO">
    <xs:sequence>
      <xs:element name="annoEsercizio" type="xs:int" minOccurs="0"/>
      <xs:element name="annoProvvisorio" type="xs:int" minOccurs="0"/>
      <xs:element name="causale" type="xs:string" minOccurs="0"/>
      <xs:element name="codiceFiscaleEnte" type="xs:string" minOccurs="0"/>
      <xs:element name="utenteInsVar" type="xs:string" minOccurs="0"/>
      <xs:element name="dataInsVar" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="dataMovimento" type="xs:dateTime" minOccurs="0"/>
      <!-- xs:element name="descrizione" type="xs:string" minOccurs="0"/> -->
      <xs:element name="id" type="xs:long" minOccurs="0"/>
      <xs:element name="identificativoFlusso" type="xs:string" minOccurs="0"/>
