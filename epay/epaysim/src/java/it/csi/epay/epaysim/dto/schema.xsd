      <xs:element name="importoProvvisorio" type="xs:decimal" minOccurs="0"/>
      <xs:element name="numeroProvvisorio" type="xs:int" minOccurs="0"/>
      <xs:element name="stato" type="xs:string" minOccurs="0"/>
      <!-- xs:element name="tipoMovimento" type="xs:string" minOccurs="0"/> -->
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="GiornaleDiCassaDTO">
