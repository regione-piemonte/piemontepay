  	  <xs:element name="xml" type="xs:string" minOccurs="0"/>
  	</xs:sequence>
  </xs:complexType>

  <xs:complexType name="testDTO">
    <xs:sequence>
      <xs:element name="descrizione" type="xs:string" minOccurs="0"/>
      <xs:element name="id" type="xs:long" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>